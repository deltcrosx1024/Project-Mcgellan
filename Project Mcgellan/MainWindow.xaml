<Window x:Class="Project_Mcgellan.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Mcgellan"
        mc:Ignorable="d"
        Title="MainWindow" Style="{DynamicResource MainWindowStyle}"
        WindowStartupLocation="CenterScreen">
    <DockPanel Background="{DynamicResource DefaultDark}">
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Options">
                <MenuItem Header="Settings" Click="SettingsMenuItem_Click"/>
            </MenuItem>
        </Menu>
    </DockPanel>
</Window>
